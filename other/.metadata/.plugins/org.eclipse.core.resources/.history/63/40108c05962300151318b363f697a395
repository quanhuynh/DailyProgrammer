
public class Cell {
	private boolean isAlive;
	private int position;
	
	public Cell(int index, boolean state) {
		position = index;
		isAlive = state;
	}
	
	public int getPos() {
		return position;
	}
	public boolean isAlive() {
		return isAlive;
	}
	public void live() {
		isAlive = true;
	}
	public void die() {
		isAlive = false;
	}
}
